<% provide(:title, 'All users') %>
<h1>All users</h1>
<div class="input-append">
  <%= form_tag users_path, method: 'get' do %>
    <%= text_field_tag :search, params[:search], class: "input-large", 
    				   							id: "appendedInputButtons", 
    				   							placeholder: "User name" %>
    <button class="btn" type="btn">Search</button>
  <% end %>
</div>

<%= will_paginate %>

<ul class="index">
  <%= render @users %>
</ul>

<%= will_paginate %>